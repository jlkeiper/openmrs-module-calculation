<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.calculation.patient">
	<class name="ExperimentalItem" table="calculation_experimental">
        <id name="experimentalItemId" type="int" column="calculation_experimental_id">
            <generator class="native">
                <param name="sequence">calculation_experimental_id_seq</param>
            </generator>
        </id>
        <discriminator column="calculation_experimental_id" insert="false" />
        <property name="patientId" type="java.lang.Integer" column="patient_id" not-null="true" />
        <property name="className" type="java.lang.String" column="class_name" length="255" />
        <property name="parameterValues" type="java.lang.String" column="parameter_values" length="512" />
        <property name="result" type="java.lang.String" length="65535" />
        <property name="dateNow" type="java.util.Date" column="date_now" length="10" not-null="true" />
        <property name="dateUpdated" type="java.util.Date" column="date_updated" length="10" not-null="true" />
    </class>
</hibernate-mapping>
